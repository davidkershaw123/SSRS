      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>6f8ad728-8cd8-48c9-b6e8-8109096e22f4</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Tropos</DataSourceName>
        <CommandText>select distinct
MBD030.SUPPLIER								as Supplier
,MBD030.SUPNAME								as Supplier_Name
,MBD010.PORDER								as PO_Number
,MBD070.PORDITM_ITEM						as PO_Line
,MBD070.PORDTYPE							as Order_Type
,MBD070.PARTNO_PORD							as Item_Code
,MBB010.[DESCRIPTION]						as Description
,MBD070.PORDQTY								as Quantity
,MBD020.GRNREC								as Delivered_Quantity
,(MBD070.PORDQTY - MBD020.GRNREC)			as Variance
,(MBD070.PORDQTY - MBD020.GRNREC) * MBD070.PORDPRICE as Variance_value
,MBD070.COSTCODE_PORD						as Cost_Code
,MBD070.COSTCENTRE_PORD						as Cost_Centre

,CASE WHEN MBD070.ORIGIN_PORD = ' ' THEN  MBD200.BUYNAME
		ELSE MBD070.ORIGIN_PORD
		END									as Originator

--Purchase Order Header
from MBD010

--Purchase Order Lines
left outer join MBD070
on MBD010.PORDER		    = MBD070.PORDER_ITEM
and MBD010.ACCOUNT15_PORD	= MBD070.ACCOUNT15_ITEM

--GRN/Invoice details
left outer join MBD020
on MBD020.PORDER_GRN		= MBD070.PORDER_ITEM
and MBD020.ACCOUNT15_GRN	= MBD070.ACCOUNT15_ITEM
and MBD020.PORDITM_GRN      = MBD070.PORDITM_ITEM

--Item details
left outer join MBB010
on MBD070.PARTNO_PORD		= MBB010.PARTNO
and MBD070.ACCOUNT15_ITEM   = MBB010.ACCOUNT15

--Purchase Buyer details
left outer join MBD200
on MBD010.ACCOUNT15_PORD = MBD200.ACCOUNT15_BUY 
and MBD010.BUYER_PORD = MBD200.BUYER

--Supplier Details
left outer join MBD030
on MBD070.ACCOUNT15_ITEM = MBD030.ACCOUNT15_SUP
and MBD010.SUPPLIER_PORD = MBD030.SUPPLIER

where
 --Order type in Inventory
  MBD070.PORDTYPE = 'I'
--All open purchase order, defined as at least 1 GRN received Quantity &gt; 0 but &lt; PO ordered Quantity
and MBD020.GRNREC &gt; 0 
and MBD020.GRNREC &lt; MBD070.PORDQTY

order by PO_Number</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>

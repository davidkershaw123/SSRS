<Report xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:cl="http://schemas.microsoft.com/sqlserver/reporting/2010/01/componentdefinition" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition">
  <Description>V1</Description>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="Tropos">
      <DataSourceReference>/Data Sources/Tropos</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>2823a240-f37f-4728-8f41-72b0408cd697</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Tropos</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CommCode">
            <Value>=Parameters!CommCode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PnCode">
            <Value>=Parameters!PnCode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ItemCode">
            <Value>=Parameters!ItemCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/* Stock Quantities report */

	SELECT        
	 MBB010.PNCLASS AS CATEGORY
	,MBB010.COMMCODE AS COMMODITY_CODE
	,MBB010.PARTNO    AS ITEM_CODE
	,MBB010.[DESCRIPTION] AS DESCRIPTION
	,MBB220.PARTNO_XRF AS SAGE_CODE
	,MBC010.UOM_STOCK AS UOM
	,ISNULL(SUM(MBC020.LOCSTOCK + MBC020.LOCPICKED + MBC020.LOCCONSIGN), 0) AS Quantity
		
--Item
FROM MBB010

--Stock balances
	LEFT JOIN MBC020
		ON  MBB010.ACCOUNT15 		= MBC020.ACCOUNT15_ALT
		AND	MBB010.PARTNO 		= MBC020.PARTNO_ALT 

--Inventory Data
	LEFT JOIN MBC010 with(readuncommitted)
		ON  MBB010.ACCOUNT15 		= MBC010.ACCOUNT15_INV
		AND MBB010.PARTNO	 	= MBC010.PARTNO_INV
		AND MBC010.WAREHOUSE 		= ' '

--Supplier Batch Records
	LEFT JOIN MBB220 with(readuncommitted)
    	ON MBB010.ACCOUNT15			= MBB220.ACCOUNT15_XRF
		AND MBB010.PARTNO		= MBB220.PARTNO_PREF
		AND MBB220.APNTYPE		= 'A'

			
	WHERE 
	MBB010.ACCOUNT15 = '000010000100001'
	AND MBB010.COMMCODE &gt; ' '
	AND MBB010.PARTNO LIKE '%' + @ItemCode + '%'
	AND MBB010.COMMCODE LIKE '%' + @CommCode + '%'
	AND MBB010.PNCLASS LIKE '%' + @PnCode + '%'
	AND MBC010.WAREHOUSE = ' '

	group by MBB010.PNCLASS,
	         MBB010.COMMCODE,
			 MBB010.PARTNO,
			 MBB010.[DESCRIPTION],
			 MBB220.PARTNO_XRF,
			 MBC010.UOM_STOCK
			 			 
	Order by MBB010.PNCLASS,
			 MBB010.COMMCODE,
			 MBB010.PARTNO</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
